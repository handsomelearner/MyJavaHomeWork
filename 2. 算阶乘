package page102works;
import java.util.Scanner;

public class Tester01 {
static int Test(int n)
{
if((n==1)||(n==0))
return 1;
else
return n*Test(n-1);
}
public static void main(String[] args) {
Scanner stdin = new Scanner(System.in);
for(int i=0;i<4;i++)
{
int x=stdin.nextInt();
System.out.println(Test(x));
}
}

}
